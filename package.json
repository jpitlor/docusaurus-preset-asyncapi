{
  "name": "docusaurus3-asyncapi-example",
  "private": true,
  "version": "1.0.3",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "docusaurus start",
    "release": "yarn build:packages && changeset publish --no-git-tag",
    "build:packages": "yarn workspaces foreach -A  run build"
  },
  "dependencies": {
    "@docusaurus/core": "3.5.2",
    "@docusaurus/plugin-debug": "^3.5.2",
    "@docusaurus/theme-classic": "^3.5.2",
    "@jpitlor/docusaurus-preset-asyncapi": "workspace:^",
    "@mdx-js/react": "^3.0.1",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@docusaurus/module-type-aliases": "3.5.2",
    "@tsconfig/docusaurus": "^2.0.3",
    "typescript": "^5.5.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "packageManager": "yarn@4.4.0"
}
